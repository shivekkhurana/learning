{
	"name": "Musejam",
	"description": "The thing for Live music industry",
	"require": {
		"laravel/framework": "4.*",
    "k/reposed": "dev-master",
    "guzzle/guzzle": "3.7.*@dev"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
    "psr-0": {
      "Abstracts": "app/src",
      "Core": "app/src",
      "Repository": "app/src",
      "Service": "app/src",
      "Tools": "app/src",
      "Overrides": "app/src",
      "Web": "app/src"
    }
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
